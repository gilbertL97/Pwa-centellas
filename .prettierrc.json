{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabWidth": 4,
  "useTabs": false,
  "printWidth": 90,
  "endOfLine": "auto",
  "singleQuote": true,
  "bracketSpacing": true,
  "arrowParets": "always",
  "proseWrap": "never",
  "htmlWhitespaceSensitivity": "strict",
  "vueIndentScriptAndStyle": true,
  "trailingComma": "all"
}